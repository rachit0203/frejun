<svg width="500" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="500" height="300" fill="#f5f5f5" rx="10" ry="10"/>
  
  <!-- Support Dashboard -->
  <rect x="40" y="30" width="420" height="240" rx="8" ry="8" fill="#fff" stroke="#ddd" stroke-width="2"/>
  <rect x="60" y="50" width="380" height="30" rx="5" ry="5" fill="#34CB65"/>
  <text x="165" y="70" font-family="Arial" font-size="14" fill="white">CUSTOMER SUPPORT DASHBOARD</text>
  
  <!-- Left Panel - Customer Queue -->
  <rect x="60" y="100" width="150" height="150" rx="5" ry="5" fill="#f9f9f9" stroke="#eee" stroke-width="1"/>
  <text x="75" y="120" font-family="Arial" font-size="12" fill="#555">Customer Queue</text>
  
  <rect x="70" y="130" width="130" height="25" rx="3" ry="3" fill="#fff" stroke="#34CB65" stroke-width="1"/>
  <circle cx="85" cy="142" r="8" fill="#34CB65"/>
  <text x="100" y="147" font-family="Arial" font-size="11" fill="#444">John Doe</text>
  
  <rect x="70" y="165" width="130" height="25" rx="3" ry="3" fill="#fff" stroke="#ddd" stroke-width="1"/>
  <circle cx="85" cy="177" r="8" fill="#ddd"/>
  <text x="100" y="182" font-family="Arial" font-size="11" fill="#444">Sarah Smith</text>
  
  <rect x="70" y="200" width="130" height="25" rx="3" ry="3" fill="#fff" stroke="#ddd" stroke-width="1"/>
  <circle cx="85" cy="212" r="8" fill="#ddd"/>
  <text x="100" y="217" font-family="Arial" font-size="11" fill="#444">Mike Johnson</text>
  
  <!-- Right Panel - Call Data -->
  <rect x="230" y="100" width="210" height="150" rx="5" ry="5" fill="#f9f9f9" stroke="#eee" stroke-width="1"/>
  <text x="280" y="120" font-family="Arial" font-size="12" fill="#555">Active Call</text>
  
  <!-- Call Timeline -->
  <line x1="250" y1="140" x2="420" y2="140" stroke="#eee" stroke-width="2"/>
  <circle cx="280" cy="140" r="6" fill="#34CB65"/>
  <circle cx="340" cy="140" r="6" fill="#34CB65"/>
  <circle cx="400" cy="140" r="6" fill="#ddd"/>
  
  <text x="265" y="160" font-family="Arial" font-size="9" fill="#777">Start</text>
  <text x="325" y="160" font-family="Arial" font-size="9" fill="#777">In Progress</text>
  <text x="390" y="160" font-family="Arial" font-size="9" fill="#777">Resolved</text>
  
  <!-- Sentiment Analysis -->
  <rect x="250" y="180" width="170" height="20" rx="3" ry="3" fill="#f0f0f0"/>
  <rect x="250" y="180" width="100" height="20" rx="3" ry="3" fill="#34CB65"/>
  <text x="270" y="195" font-family="Arial" font-size="10" fill="white">Positive Sentiment</text>
  
  <!-- Action Buttons -->
  <rect x="250" y="210" width="80" height="25" rx="4" ry="4" fill="#34CB65"/>
  <text x="270" y="227" font-family="Arial" font-size="11" fill="white">Resolve</text>
  
  <rect x="340" y="210" width="80" height="25" rx="4" ry="4" fill="#f0f0f0" stroke="#ddd" stroke-width="1"/>
  <text x="355" y="227" font-family="Arial" font-size="11" fill="#555">Transfer</text>
</svg> 